ExecStart=/usr/sbin/daemonize -p /run/gnmi.pid /usr/local/bin/gnmi run --address 0.0.0.0 --port 5002 -v --config /var/run/redis/sonic-db/database_config.json --username admin --password YourPaSsWoRd
